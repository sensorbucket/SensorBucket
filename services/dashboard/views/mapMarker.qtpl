{% func MapMarker(deviceID int64, latitude, longitude float64, label string) %}
<div id="markers" hx-swap-oob="beforeend">
    <map-marker
        id="{%dl deviceID %}"
        latitude="{%f latitude %}"
        longitude="{%f longitude %}"
        label="{%s label %}"
        hx-get="{%s u("/overview/devices/%d", deviceID) %}"
        hx-target="main"
        hx-push-url="true"
    >
    </map-marker>
</div>
{% endfunc %}
