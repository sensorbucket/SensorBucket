{% import "sensorbucket.nl/sensorbucket/services/core/devices" %}

{% func SensorGroupSearch(sgs []devices.SensorGroup) %}
    {% for _, sg := range sgs %}
        <li>
            <a
                href="?sensorgroup={%dl sg.ID %}"
                class="block hover:bg-primary-100 p-2"
            >{%s sg.Name %}</a>
        </li>
    {% endfor %}
{% endfunc %}
