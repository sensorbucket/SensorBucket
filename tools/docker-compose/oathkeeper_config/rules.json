[
    {
        "id": "passthrough-authentication",
        "match": {
            "url": "http://<127.0.0.1|localhost>:3000/<\\.ory(/.+)?>",
            "methods": [
                "GET","POST","PATCH","PUT","DELETE"
            ]
        },
        "authenticators": [
            {
                "handler": "noop"
            }
        ],
        "authorizer": {
            "handler": "allow"
        },
        "mutators": [{"handler":"noop"}]
    },
    {
        "id": "user_preferences",
        "match": {
            "url": "http://<127.0.0.1|localhost>:3000/tenants/switch",
            "methods": [
                "GET","POST","PATCH","PUT","DELETE"
            ]
        },
        "authenticators": [
            {
                "handler": "cookie_session"
            }
        ],
        "authorizer": {
            "handler": "allow"
        },
        "mutators": [
            {
                "handler": "hydrator"
            },
            {
                "handler": "id_token"
            }
        ]
    },
    {
        "id": "authentication",
        "match": {
            "url": "http://<127.0.0.1|localhost>:3000/<tenants(/(auth|api-keys|static)(/.+)?)?>",
            "methods": [
                "GET","POST","PATCH","PUT","DELETE"
            ]
        },
        "authenticators": [
            {
                "handler": "noop"
            }
        ],
        "authorizer": {
            "handler": "allow"
        },
        "mutators": [
            {
                "handler": "hydrator"
            },
            {
                "handler": "id_token",
                "config": {}
            }
        ]
    },
    {
        "id": "default",
        "match": {
            "url": "http://<127.0.0.1|localhost>:3000/<(dashboard|api)(/.*)?>",
            "methods": [
                "GET","POST","PATCH","PUT","DELETE"
            ]
        },
        "authenticators": [
            {
                "handler": "cookie_session"
            },
            {
                "handler": "noop"
            }
        ],
        "authorizer": {
            "handler": "allow"
        },
        "mutators": [
            {
                "handler": "hydrator"
            },
            {
                "handler": "id_token",
                "config": {}
            }
        ]
    },
    {
        "id": "internal",
        "match": {
            "url": "http://<127.0.0.1|localhost>:3000/<(dev)(/.*)?>",
            "methods": [
                "GET","POST","PATCH","PUT","DELETE"
            ]
        },
        "authenticators": [
            {
                "handler": "noop"
            }
        ],
        "authorizer": {
            "handler": "allow"
        },
        "mutators": [
            {
                "handler": "noop"
            }
        ]
    },
    {
        "id": "internal-docker",
        "match": {
            "url": "http://<(?!localhost|127.0.0.1).*>",
            "methods": [
                "GET","POST","PATCH","PUT","DELETE"
            ]
        },
        "authenticators": [
            {
                "handler": "noop"
            }
        ],
        "authorizer": {
            "handler": "allow"
        },
        "mutators": [
            {
                "handler": "noop"
            }
        ]
    }
]
