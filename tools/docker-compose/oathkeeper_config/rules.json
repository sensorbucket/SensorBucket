[
    {
        "id": "passthrough-authentication",
        "match": {
            "url": "http://<127.0.0.1|localhost>:3000/<(\\.ory|tenants/auth/settings|tenants/auth/login|tenants/auth/logout|tenants/static|dev)(/.+)?>",
            "methods": [
                "GET","POST","PATCH","PUT","DELETE"
            ]
        },
        "authenticators": [
            {
                "handler": "noop"
            }
        ],
        "authorizer": {
            "handler": "allow"
        },
        "mutators": [{"handler":"noop"}]
    },
    {
        "id": "auth",
        "match": {
            "url": "http://<127.0.0.1|localhost>:3000/<tenants/auth|((tenants/switch|tenants/api-keys|dashboard|api)(/.*)?)>",
            "methods": [
                "GET",
                "POST",
                "PATCH",
                "PUT",
                "DELETE"
            ]
        },
        "authenticators": [
            {
                "handler": "cookie_session"
            },
            {
                "handler": "bearer_token"
            }
        ],
        "authorizer": {
            "handler": "allow"
        },
        "mutators": [
            {
                "handler": "hydrator"
            },
            {
                "handler": "id_token"
            }
        ],
        "errors": [
            {
                "handler": "redirect"
            }
        ]
    },
    {
        "id": "auth",
        "match": {
            "url": "http://caddy<.*>",
            "methods": [
                "GET",
                "POST",
                "PATCH",
                "PUT",
                "DELETE"
            ]
        },
        "authenticators": [
            {
                "handler": "noop"
            }
        ],
        "authorizer": {
            "handler": "allow"
        },
        "mutators": [
            {
                "handler": "noop"
            }
        ]
    }
]
