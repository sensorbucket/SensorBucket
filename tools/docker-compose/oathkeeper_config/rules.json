[
    {
        "id": "passthrough-authentication",
        "match": {
            "url": "http://<127.0.0.1|localhost>:3000/<\\.ory(/.+)?>",
            "methods": [
                "GET","POST","PATCH","PUT","DELETE"
            ]
        },
        "authenticators": [
            {
                "handler": "noop"
            }
        ],
        "authorizer": {
            "handler": "allow"
        },
        "mutators": [{"handler":"noop"}]
    },
    {
        "id": "authentication",
        "match": {
            "url": "http://<127.0.0.1|localhost>:3000/<tenants/auth(/.+)?|tenants/static/.*>",
            "methods": [
                "GET","POST","PATCH","PUT","DELETE"
            ]
        },
        "authenticators": [
            {
                "handler": "noop"
            }
        ],
        "authorizer": {
            "handler": "allow"
        },
        "mutators": [
            {
                "handler": "id_token",
                "config": {}
            }
        ]
    },
    {
        "id": "default",
        "match": {
            "url": "http://<127.0.0.1|localhost>:3000/<dashboard|api|dev>/<.*>",
            "methods": [
                "GET","POST","PATCH","PUT","DELETE"
            ]
        },
        "authenticators": [
            {
                "handler": "cookie_session"
            },
            {
                "handler": "noop"
            }
        ],
        "authorizer": {
            "handler": "allow"
        },
        "mutators": [
            {
                "handler": "id_token",
                "config": {}
            }
        ]
    }
]
